<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">
    <changeSet id="7643354323000111" author="Bartek">
        <createTable tableName="user_role">
            <column name="user_id" type="bigint">
                <constraints nullable="false"/>
            </column>
            <column name="role_id" type="bigint">
                <constraints nullable="false"/>
            </column>
        </createTable>
        <addForeignKeyConstraint baseTableName="user_role" baseColumnNames="user_id" constraintName="user_role_fx" referencedTableName="user"
                                 referencedColumnNames="id"/>
        <addForeignKeyConstraint baseTableName="user_role" baseColumnNames="role_id" constraintName="role_user_fk" referencedTableName="role"
                                 referencedColumnNames="id"/>
    </changeSet>
</databaseChangeLog>